---------------------------3.10 Programming Exercises, Pick any 1 exercise
.386
model flat,stdcall
.stack 4096
ExitProcess proto
.code
main proc
	mov eax, 1
  mov ebx, 2
  mov ecx, 3
  mov edx, 4
  ADD ebx, eax
  ADD ecx, edx 
  SUB ebx, ecx
  MOV eax, 0
  MOV eax,ebx 
	

	call ExitProcess
main endp
end

















